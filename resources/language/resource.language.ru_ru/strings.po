msgctxt "#32000"
msgid "RSS Editor"
msgstr "Редактор RSS"

msgctxt "#32001"
msgid "Change Set"
msgstr "Изменить набор"

msgctxt "#32002"
msgid "items"
msgstr "элементы"

msgctxt "#32003"
msgid "Pages"
msgstr "Страницы"

msgctxt "#32006"
msgid "Edit Attributes"
msgstr "Изменить атрибуты"

msgctxt "#32012"
msgid "RSS Feed URL"
msgstr "Ссылка на ленту RSS"

msgctxt "#32013"
msgid "Update Interval"
msgstr "Интервал обновления"

msgctxt "#32014"
msgid "Enter the paths of your RSS feeds %s"
msgstr "Введите пути к RSS лентам %s"

msgctxt "#32024"
msgid "Select a Set "
msgstr "Выбор набора "

msgctxt "#32025"
msgid "Set ID"
msgstr "Задать ID"

msgctxt "#32027"
msgid "Enable Right to Left Text for this Set?"
msgstr "Включить текст справа налево для этого набора?"

msgctxt "#32030"
msgid "Set Editor"
msgstr "Задать редактор"

msgctxt "#32040"
msgid "Error: "
msgstr "Ошибка: "

msgctxt "#32041"
msgid "was either not found in the system,"
msgstr "либо не был найден в системе,"

msgctxt "#32042"
msgid "or does not contain parsable XML."
msgstr "или не содержит обрабатываемый XML."

msgctxt "#32043"
msgid "Try deleting it from your userdata folder and restarting XBMC."
msgstr "Попробуйте удалить это из папки данных пользователя и перезапустите Kodi."

msgctxt "#32045"
msgid "Cannot Delete Set 1"
msgstr "Не удалось удалить набор 1"

msgctxt "#32046"
msgid "Set 1 is required.  You cannot delete this."
msgstr "Требуется 1-ый набор. Вы не можете это удалить."

msgctxt "#32047"
msgid "Reset set 1 to default?"
msgstr "Сбросить 1-ый набор по умолчанию?"

msgctxt "#32050"
msgid "Set %s already exists."
msgstr "Набор %s уже существует."

msgctxt "#32051"
msgid "Your RSSFeeds.xml file is corrupt."
msgstr "Фвйл RSSFeeds.xml поврежден."

msgctxt "#32052"
msgid "Attempt to regenerate"
msgstr "Попытка восстановления"

msgctxt "#32053"
msgid "You will loose all previous settings"
msgstr "Будут потеряны все прошлые настройки"
