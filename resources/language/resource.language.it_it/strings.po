msgstr "Uno script per modificare lo scorrimento RSS integrato di Kodi"

msgctxt "Addon Description"
msgid "A Script for editing Kodi's built in RSS Ticker"
msgstr "Uno script per modificare lo scorrimento RSS integrato di Kodi"

msgctxt "#32000"
msgid "RSS Editor"
msgstr "Editore RSS"

msgctxt "#32001"
msgid "Change Set"
msgstr "Cambia set"

msgctxt "#32002"
msgid "items"
msgstr "elementi"

msgctxt "#32003"
msgid "Pages"
msgstr "Pagine"

msgctxt "#32006"
msgid "Edit Attributes"
msgstr "Modifica attributi"

msgctxt "#32012"
msgid "RSS Feed URL"
msgstr "URLfeed RSS"

msgctxt "#32013"
msgid "Update Interval"
msgstr "Intervallo aggiornamento"

msgctxt "#32014"
msgid "Enter the paths of your RSS feeds %s"
msgstr "Inserisci i percorsi dei tuoi feed RSS %s"

msgctxt "#32024"
msgid "Select a Set "
msgstr "Seleziona set "

msgctxt "#32025"
msgid "Set ID"
msgstr "Imposta ID"

msgctxt "#32027"
msgid "Enable Right to Left Text for this Set?"
msgstr "Abilitare il testo da destra a sinistra per questo set?"

msgctxt "#32030"
msgid "Set Editor"
msgstr "Imposta editore"

msgctxt "#32040"
msgid "Error: "
msgstr "Errore: "

msgctxt "#32041"
msgid "was either not found in the system,"
msgstr "non è stato trovato nel sistema,"

msgctxt "#32042"
msgid "or does not contain parsable XML."
msgstr "o non contiene XML analizzabile."

msgctxt "#32043"
msgid "Try deleting it from your userdata folder and restarting XBMC."
msgstr "Prova a eliminarlo dalla cartella dei dati utente e a riavviare Kodi."

msgctxt "#32045"
msgid "Cannot Delete Set 1"
msgstr "Impossibile eliminare il set 1"

msgctxt "#32046"
msgid "Set 1 is required.  You cannot delete this."
msgstr "Il set 1 è obbligatorio.   Non puoi eliminarlo."

msgctxt "#32047"
msgid "Reset set 1 to default?"
msgstr "Reimpostare il set 1 al valore predefinito?"

msgctxt "#32050"
msgid "Set %s already exists."
msgstr "Il set %s esiste già."

msgctxt "#32051"
msgid "Your RSSFeeds.xml file is corrupt."
msgstr "Il tuo file RSSFeeds.xml è danneggiato."

msgctxt "#32052"
msgid "Attempt to regenerate"
msgstr "Tento di rigenerare"

msgctxt "#32053"
msgid "You will loose all previous settings"
msgstr "Perderai tutte le impostazioni precedenti"
