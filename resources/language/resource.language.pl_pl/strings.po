msgctxt "#32000"
msgid "RSS Editor"
msgstr "Edytor RSS"

msgctxt "#32001"
msgid "Change Set"
msgstr "Zmień zestaw"

msgctxt "#32002"
msgid "items"
msgstr "pozycji"

msgctxt "#32003"
msgid "Pages"
msgstr "Strony"

msgctxt "#32006"
msgid "Edit Attributes"
msgstr "Edytuj atrybuty"

msgctxt "#32012"
msgid "RSS Feed URL"
msgstr "Adres URL kanału RSS"

msgctxt "#32013"
msgid "Update Interval"
msgstr "Częstotliwość aktualizacji"

msgctxt "#32014"
msgid "Enter the paths of your RSS feeds %s"
msgstr "Wprowadź ścieżki swoich kanałów RSS %s"

msgctxt "#32024"
msgid "Select a Set "
msgstr "Wybierz zestaw "

msgctxt "#32025"
msgid "Set ID"
msgstr "Ustaw identyfikator"

msgctxt "#32027"
msgid "Enable Right to Left Text for this Set?"
msgstr "Włączyć tekst od prawej do lewej dla tego zestawu?"

msgctxt "#32030"
msgid "Set Editor"
msgstr "Ustaw edytor"

msgctxt "#32040"
msgid "Error: "
msgstr "Błąd: "

msgctxt "#32041"
msgid "was either not found in the system,"
msgstr "albo nie został znaleziony w systemie,"

msgctxt "#32042"
msgid "or does not contain parsable XML."
msgstr "lub nie zawiera parsowalnego kodu XML."

msgctxt "#32043"
msgid "Try deleting it from your userdata folder and restarting XBMC."
msgstr "Spróbuj usunąć go z folderu danych użytkownika i ponownie uruchomić XBMC."

msgctxt "#32045"
msgid "Cannot Delete Set 1"
msgstr "Nie można usunąć zestawu 1"

msgctxt "#32046"
msgid "Set 1 is required.  You cannot delete this."
msgstr "Zestaw 1 jest wymagany. Nie możesz go usunąć."

msgctxt "#32047"
msgid "Reset set 1 to default?"
msgstr "Zresetować zestaw 1 do wartości domyślnych?"

msgctxt "#32050"
msgid "Set %s already exists."
msgstr "Zestaw %s już istnieje."

msgctxt "#32051"
msgid "Your RSSFeeds.xml file is corrupt."
msgstr "Twój plik RSSFeeds.xml jest uszkodzony."

msgctxt "#32052"
msgid "Attempt to regenerate"
msgstr "Próba regeneracji"

msgctxt "#32053"
msgid "You will loose all previous settings"
msgstr "Utracisz wszystkie poprzednie ustawienia"
