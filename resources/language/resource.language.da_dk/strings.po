msgctxt "Addon Summary"
msgid "A Script for editing Kodi's built in RSS Ticker"
msgstr "Et script til redigering af Kodis indbyggede RSS-ticker"

msgctxt "Addon Description"
msgid "A Script for editing Kodi's built in RSS Ticker"
msgstr "Et script til redigering af Kodis indbyggede RSS-ticker"

msgctxt "#32000"
msgid "RSS Editor"
msgstr "RSS Editor"

msgctxt "#32001"
msgid "Change Set"
msgstr "Ændring af sæt"

msgctxt "#32002"
msgid "items"
msgstr "emner"

msgctxt "#32003"
msgid "Pages"
msgstr "Sider"

msgctxt "#32006"
msgid "Edit Attributes"
msgstr "Rediger egenskaber"

msgctxt "#32012"
msgid "RSS Feed URL"
msgstr "URL til RSS feed"

msgctxt "#32013"
msgid "Update Interval"
msgstr "Opdateringsinterval"

msgctxt "#32014"
msgid "Enter the paths of your RSS feeds %s"
msgstr "Indtast stierne til dine RSS feeds %s"

msgctxt "#32024"
msgid "Select a Set "
msgstr "Vælg et sæt "

msgctxt "#32025"
msgid "Set ID"
msgstr "Sæt ID"

msgctxt "#32027"
msgid "Enable Right to Left Text for this Set?"
msgstr "Aktiver tekst fra højre mod venstre for dette sæt?"

msgctxt "#32030"
msgid "Set Editor"
msgstr "Redigering af sæt"

msgctxt "#32040"
msgid "Error: "
msgstr "Fejl: "

msgctxt "#32041"
msgid "was either not found in the system,"
msgstr "blev enten ikke fundet i systemet,"

msgctxt "#32042"
msgid "or does not contain parsable XML."
msgstr "eller indeholder ikke læselig XML."

msgctxt "#32043"
msgid "Try deleting it from your userdata folder and restarting XBMC."
msgstr "Prøv at slette det fra din brugerdata-mappe og genstart Kodi."

msgctxt "#32045"
msgid "Cannot Delete Set 1"
msgstr "Kan ikke slette sæt 1"

msgctxt "#32046"
msgid "Set 1 is required.  You cannot delete this."
msgstr "Sæt 1 er påkrævet.  Du kan ikke slette dette."

msgctxt "#32047"
msgid "Reset set 1 to default?"
msgstr "Nulstil sæt 1 til standard?"

msgctxt "#32050"
msgid "Set %s already exists."
msgstr "Sæt %s eksisterer allerede."

msgctxt "#32051"
msgid "Your RSSFeeds.xml file is corrupt."
msgstr "Din RSSFeeds.xml-fil er korrupt."

msgctxt "#32052"
msgid "Attempt to regenerate"
msgstr "Forsøger at regenerere"

msgctxt "#32053"
msgid "You will loose all previous settings"
msgstr "Du mister alle tidligere indstillinger"
