msgstr "Set 1 kann nicht gelöscht werden"

msgctxt "#32046"
msgid "Set 1 is required.  You cannot delete this."
msgstr "Set 1 wird immer benötigt. Es kann nicht gelöscht werden."

msgctxt "#32047"
msgid "Reset set 1 to default?"
msgstr "Set 1 auf Standardwerte zurücksetzen?"

msgctxt "#32050"
msgid "Set %s already exists."
msgstr "Set %1 existiert bereits."

msgctxt "#32051"
msgid "Your RSSFeeds.xml file is corrupt."
msgstr "Die Datei RSSFeeds.xml ist defekt."

