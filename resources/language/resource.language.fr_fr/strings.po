msgctxt "#32001"
msgid "Change Set"
msgstr "Modifier l'ensemble"

msgctxt "#32002"
msgid "items"
msgstr "éléments"

msgctxt "#32003"
msgid "Pages"
msgstr "Pages"

msgctxt "#32006"
msgid "Edit Attributes"
msgstr "Modifier les attributs"

msgctxt "#32012"
msgid "RSS Feed URL"
msgstr "URL du flux RSS"

msgctxt "#32013"
msgid "Update Interval"
msgstr "Intervalle de mise à jour"

msgctxt "#32014"
msgid "Enter the paths of your RSS feeds %s"
msgstr "Entrer les chemins de vos flux RSS %s"

msgctxt "#32024"
msgid "Select a Set "
msgstr "Sélectionner un ensemble "

msgctxt "#32025"
msgid "Set ID"
msgstr "Définir l'identifiant"

